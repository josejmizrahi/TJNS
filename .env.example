# App
NODE_ENV=development
PORT=3000
API_PREFIX=/api/v1

# Database
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=jns_user
POSTGRES_PASSWORD=your_password
POSTGRES_DB=jns_db

# MongoDB
MONGODB_URI=mongodb://localhost:27017/jns

# JWT
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=24h

# XRPL
XRPL_NODE=wss://s.altnet.rippletest.net:51233
XRPL_COLD_WALLET=your_cold_wallet
XRPL_HOT_WALLET=your_hot_wallet
XRPL_HOT_WALLET_SEED=your_hot_wallet_seed

# IPFS
IPFS_NODE=http://localhost:5001
IPFS_GATEWAY=http://localhost:8080

# RabbitMQ
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_QUEUE_PREFIX=jns

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Security
ENCRYPTION_KEY=your_encryption_key
ENCRYPTION_IV=your_encryption_iv
